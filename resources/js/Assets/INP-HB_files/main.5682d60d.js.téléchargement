arit\xe9 et la performance."]})]}),(0,Sn.jsx)(MQ,{className:"pb-3 pt-5 mb-2",fluid:!0,"data-aos":"fade-up",children:(0,Sn.jsxs)(ns,{children:[(0,Sn.jsx)("p",{className:"body-highlight text-center text-light text-uppercase fw-bold pt-0 mt-0",children:"meilleurs joueurs"}),(0,Sn.jsx)(mQ,{eleves:DQ})]})}),(0,Sn.jsxs)(ns,{children:[(0,Sn.jsx)("div",{className:"text-center text-uppercase fs-2 my-3",children:"Nos diff\xe9rentes aires de jeu"}),(0,Sn.jsx)(jQ,{})]})]})};function TQ(){var e=(0,a.useRef)(!1);return os((function(){return e.current=!0,function(){e.current=!1}}),[]),e}var OQ=function(e){j(n,e);var t=R(n);function n(){return g(this,n),t.apply(this,arguments)}return x(n,[{key:"getSnapshotBeforeUpdate",value:function(e){var t=this.props.childRef.current;if(t&&e.isPresent&&!this.props.isPresent){var n=this.props.sizeRef.current;n.height=t.offsetHeight||0,n.width=t.offsetWidth||0,n.top=t.offsetTop,n.left=t.offsetLeft}return null}},{key:"componentDidUpdate",value:function(){}},{key:"render",value:function(){return this.props.children}}]),n}(a.Component);function CQ(e){var t=e.children,n=e.isPresent,i=(0,a.useId)(),r=(0,a.useRef)(null),s=(0,a.useRef)({width:0,height:0,top:0,left:0});return(0,a.useInsertionEffect)((function(){var e=s.current,t=e.width,a=e.height,o=e.top,l=e.left;if(!n&&r.current&&t&&a){r.current.dataset.motionPopId=i;var c=document.createElement("style");return document.head.appendChild(c),c.sheet&&c.sheet.insertRule('\n          [data-motion-pop-id="'.concat(i,'"] {\n            position: absolute !important;\n            width: ').concat(t,"px !important;\n            height: ").concat(a,"px !important;\n            top: ").concat(o,"px !important;\n            left: ").concat(l,"px !important;\n          }\n        ")),function(){document.head.removeChild(c)}}}),[n]),a.createElement(OQ,{isPresent:n,childRef:r,sizeRef:s},a.cloneElement(t,{ref:r}))}var GQ=function(e){var t=e.children,n=e.initial,i=e.isPresent,r=e.onExitComplete,s=e.custom,o=e.presenceA